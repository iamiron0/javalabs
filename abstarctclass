class Rectangle extends Shape
{
	int side;
	Rectangle(int side)
	{
		this.side=side;
	}
	public void numberOfSide()
	{
	System.out.println("I am rectangle,number of side="+side);
	}
}

class Triangle extends Shape
{
    int side;
    Triangle(int side)
    {
	 this.side=side;
    }
    public  void  numberOfSide()
   {
	  System.out.println("I am Triangle, number of side="+side);
   }
 }

class Hexagon extends Shape
{
	int side;
	Hexagon(int side)
	{
		this.side=side;
	}
	 public  void  numberOfSide()
	   {
		  System.out.println("I am Hexagon, number of side="+side);
	   }
	 }

public class AbstractClass {

	public static void main(String args[]) {
		
	Rectangle r =new Rectangle(4);
	Triangle t =new Triangle(3);
	Hexagon h =new Hexagon(6);
	r.numberOfSide();
	t.numberOfSide();
	h.numberOfSide();
	}
}
	
	
